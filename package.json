{
    "name": "animalese-typing-sounds",
    "publisher": "SkyDotBit",
    "displayName": "Animalese Typing Sounds",
    "description": "Heheh you have no idea..",
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.40.0"
    },
    "homepage": "https://skydevs.me",
    "repository": {
        "type": "git",
        "url": "https://github.com/SkyDotBit/Animalese-for-vscode"
    },
    "keywords": [
        "audio",
        "sound",
        "play sound",
        "typing",
        "typing sound",
        "keypress",
        "keypress sound",
        "keyboard",
        "Keyboard Sounds",
        "Aural Coding",
        "Animal Crossing",
        "Animalese"
    ],
    "icon": "icon.png",
    "license": "Apache-2.0",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "typing_sounds.enable",
                "title": "Typing Sounds: Enable"
            },
            {
                "command": "typing_sounds.disable",
                "title": "Typing Sounds: Disable"
            },
            {
                "command": "typing_sounds.volumeUp",
                "title": "Typing Sounds: Volume Up"
            },
            {
                "command": "typing_sounds.volumeDown",
                "title": "Typing Sounds: Volume Down"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/fluent-ffmpeg": "^2.1.27",
        "@types/glob": "^7.1.1",
        "@types/lodash.debounce": "^4.0.6",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/play-sound": "^1.1.2",
        "@types/vscode": "^1.40.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "tslint": "^5.20.0",
        "typescript": "^5.8.3",
        "vscode": "^1.1.37",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "fluent-ffmpeg": "^2.1.3",
        "generator-code": "^1.11.10",
        "lodash.debounce": "^4.0.8",
        "play-sound": "^1.1.6",
        "yo": "^5.1.0"
    }
}
